	<queryString language="SQL">
		<![CDATA[SELECT
supplier.supplier_tin,supplier.supplier_name,
transheader.description,transheader.gross_amount,transheader.amount_exc_tax, transheader.supplier_amount,transheader.vat_amount, transheader.iwt_amount, transheader.currency,transheader.tran_date, transheader.tran_id, transheader.invoice_no,transheader.invoice_date, transheader.posted_by, transheader.verified_by, transheader.collection_date, transheader.collect_acrual_trans_id
FROM supplier
JOIN  transheader on supplier.id = transheader.supplier_id
WHERE
transheader.supplier_id IS NOT NULL
AND transheader.tran_date BETWEEN $P{fromDate} AND  $P{toDate}
AND COALESCE(transheader.transactiontype,'') LIKE  $P{transactiontype}
AND COALESCE(transheader.finacle_status,'') LIKE   $P{finacle_status}
AND COALESCE(transheader.supplier_id,'') LIKE   $P{supplier_id}]]>
	</queryString>

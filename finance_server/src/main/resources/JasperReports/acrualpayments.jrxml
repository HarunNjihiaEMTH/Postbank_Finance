    <queryString language="SQL">
        <![CDATA[SELECT
supplier.supplier_tin,supplier.supplier_name,
transheader.description,transheader.gross_amount,transheader.net_amount, transheader.supplier_amount,transheader.vat_amount, transheader.iwt_amount, transheader.currency,transheader.tran_date, transheader.tran_id, transheader.invoice_no,transheader.invoice_date, transheader.posted_by, transheader.verified_by, transheader.collection_date, transheader.collect_acrual_trans_id,
transheader.vat_rate, transheader.rate, supplier.supplier_country, transheader.tax_rate, transheader.iwt_account, transheader.iwt_amount, transheader.variance,
transheader.accrual_account, transheader.total_debit_amt
FROM supplier
JOIN  transheader on supplier.id = transheader.supplier_id
WHERE
transheader.supplier_id IS NOT NULL
AND transheader.tran_date BETWEEN $P{fromDate} AND  $P{toDate}
AND transheader.transactiontype LIKE  $P{transactiontype}
AND transheader.finacle_status LIKE   $P{finacle_status}
AND transheader.status LIKE $P{status}
AND transheader.supplier_id LIKE  $P{supplier_id}]]>
    </queryString>
